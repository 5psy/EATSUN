<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scroll2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#66B2B2B2">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/back6"
                android:layout_width="53dp"
                android:layout_height="45dp"
                android:layout_marginLeft="18dp"
                app:srcCompat="?attr/actionModeCloseDrawable" />

            <TextView
                android:id="@+id/한식1"
                android:layout_width="100dp"
                android:layout_height="45dp"
                android:layout_marginLeft="180dp"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/malgun"
                android:gravity="center"
                android:text="한식"
                android:textColor="#171616"
                android:textSize="34sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/부대찌개사진"
                        android:layout_width="100dp"
                        android:layout_height="150dp"
                        android:layout_marginLeft="18dp"
                        android:layout_marginTop="45dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/korean_food_1" />

                    <ImageView
                        android:id="@+id/알밥사진"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_marginTop="70dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/korean_food_2" />

                    <ImageView
                        android:id="@+id/닭고기덮밥사진"
                        android:layout_width="100dp"
                        android:layout_height="120dp"
                        android:layout_marginTop="60dp"
                        android:layout_marginRight="18dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/korean_food_3" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/부대찌개"
                        android:layout_width="102dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="19dp"
                        android:layout_marginTop="180dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="부대찌개"
                        android:textColor="#090909"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/알밥"
                        android:layout_width="109dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="6dp"
                        android:layout_marginTop="180dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="알밥"
                        android:textColor="#090909"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/닭고기덮밥"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="180dp"
                        android:layout_marginRight="18dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="닭고기 덮밥"
                        android:textColor="#090909"
                        android:textSize="20sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/부대찌개가격"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="200dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="5900원"
                        android:textColor="#090909"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/알밥가격"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="200dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="5900원"
                        android:textColor="#090909"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/닭고기덮밥가격"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="200dp"
                        android:layout_marginRight="18dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="5900원"
                        android:textColor="#090909"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/고기국밥사진"
                        android:layout_width="100dp"
                        android:layout_height="120dp"
                        android:layout_marginLeft="18dp"
                        android:layout_marginTop="220dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/korean_food_4" />

                    <ImageView
                        android:id="@+id/불고기덮밥사진"
                        android:layout_width="100dp"
                        android:layout_height="120dp"
                        android:layout_marginTop="220dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/korean_food_5" />

                    <ImageView
                        android:id="@+id/명란마요덮밥사진"
                        android:layout_width="100dp"
                        android:layout_height="120dp"
                        android:layout_marginTop="220dp"
                        android:layout_marginRight="18dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/korean_food_6" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/고기국밥"
                        android:layout_width="95dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="19dp"
                        android:layout_marginTop="340dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="고기 국밥"
                        android:textColor="#090909"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/불고기덮밥"
                        android:layout_width="113dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="340dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="불고기 덮밥"
                        android:textColor="#090909"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/명란마요덮밥"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="17dp"
                        android:layout_marginTop="340dp"
                        android:layout_marginRight="18dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="명란마요 덮밥"
                        android:textColor="#090909"
                        android:textSize="20sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/고기국밥가격"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="370dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="5900원"
                        android:textColor="#090909"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/불고기덮밥가격"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="370dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="6500원"
                        android:textColor="#090909"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/명란마요덮밥가격"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="370dp"
                        android:layout_marginRight="18dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="6500원"
                        android:textColor="#090909"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/명란알밥사진"
                        android:layout_width="102dp"
                        android:layout_height="120dp"
                        android:layout_marginLeft="18dp"
                        android:layout_marginTop="390dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/korean_food_7" />

                    <ImageView
                        android:id="@+id/한식뷔페사진"
                        android:layout_width="100dp"
                        android:layout_height="120dp"
                        android:layout_marginTop="390dp"
                        android:layout_marginRight="18dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/korean_food_8" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/명란알밥"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="19dp"
                        android:layout_marginTop="510dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="명란알밥"
                        android:textColor="#090909"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/한식뷔페"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="510dp"
                        android:layout_marginRight="18dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="한식 뷔페"
                        android:textColor="#090909"
                        android:textSize="20sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/명란알밥가격"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="540dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="6900원"
                        android:textColor="#080808"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/한식뷔페가격"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_marginTop="540dp"
                        android:layout_marginRight="18dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/malgunbd"
                        android:gravity="center"
                        android:text="5000원"
                        android:textColor="#090909"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="10dp"
                    android:background="#66B2B2B2"
                    android:backgroundTint="#66B2B2B2"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/한식2페이지가기"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="18dp"
                        android:layout_marginTop="650dp"
                        android:layout_marginRight="18dp"
                        android:layout_weight="1"
                        android:background="#66292323"
                        app:srcCompat="@android:drawable/ic_media_ff" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/결제"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="18dp"
                    android:layout_marginTop="680dp"
                    android:layout_marginRight="18dp"
                    android:layout_weight="1"
                    android:background="#66B2B2B2"
                    android:backgroundTint="#B3B2B2B2"
                    android:text="결제"
                    android:textSize="20sp"
                    android:textStyle="bold" />
            </LinearLayout>

        </RelativeLayout>
    </LinearLayout>

</ScrollView>